{
    "name": "marcosper/copyscape-api",
    "description": "PHP implementation of CopyScape premium API",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Per",
            "homepage": "http://github.com/marcosper"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "6.*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "phpunit/phpunit-mock-objects": "~3.0",
        "illuminate/support": "^5.3"
    },
    "autoload": {
        "psr-4": {
